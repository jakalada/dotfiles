{
  // vim key mappigns
  "vim.insertModeKeyBindings": [],
  "vim.insertModeKeyBindingsNonRecursive": [],
  "vim.normalModeKeyBindings": [],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<Leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<Esc>"],
      "commands": [
        {
          "command": ":nohl"
        }
      ]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["L"],
      "after": ["g", "_"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["<CR>"],
      "after": ["<C-]>"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["code-runner.run"]
    },
    {
      "before": ["v"],
      "after": ["v", "h", "l"]
    }
  ],
  "vim.visualModeKeyBindings": [],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["L"],
      "after": ["g", "_"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  // vim settings
  "vim.leader": " ",
  "vim.cursorStylePerMode.insert": "line-thin",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "block-outline",
  "vim.cursorStylePerMode.visual": "line-thin",
  "vim.cursorStylePerMode.visualblock": "line-thin",
  "vim.cursorStylePerMode.visualline": "line-thin",
  "vim.changeWordIncludesWhitespace": true,
  "vim.easymotionKeys": "asdfjkl",
  "vim.easymotionMarkerBackgroundColor": "#004d40",
  "vim.easymotionMarkerFontFamily": "Cica",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.easymotionMarkerForegroundColorOneChar": "#e0f2f1",
  "vim.easymotionMarkerForegroundColorTwoChar": "#00796b",
  "vim.gdefault": true,
  "vim.highlightedyank.enable": true,
  "vim.history": 100,
  "vim.hlsearch": true,
  "vim.statusBarColorControl": true,
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "workbench.colorCustomizations": {
    "[Tomorrow]": {
      "editor.background": "#f7f7f7"
    },
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f",
    "statusBar.foreground": "#ffffff",
    "statusBar.debuggingForeground": "#ffffff"
  },
  "vim.autoSwitchInputMethod.enable": true,
  "vim.autoSwitchInputMethod.defaultIM": "com.apple.keylayout.US",
  "vim.autoSwitchInputMethod.obtainIMCmd": "/usr/local/bin/im-select",
  "vim.autoSwitchInputMethod.switchIMCmd": "/usr/local/bin/im-select {im}",
  "editor.fontFamily": "Cica",
  "files.autoSave": "afterDelay",
  "editor.renderWhitespace": "selection",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "markdown.extension.toc.githubCompatibility": true,
  "editor.formatOnType": true,
  "git.autofetch": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "workbench.startupEditor": "newUntitledFile",
  "code-runner.executorMap": {
    "kotlin": "kotlinc -script"
  },
  "git.enableSmartCommit": true,
  "editor.letterSpacing": 1.25,
  "editor.lineHeight": 22,
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "[markdown]": {
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 200,
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "json.schemas": [],
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "eslint.validate": ["typescript", "typescriptreact"],
  "editor.accessibilitySupport": "off",
  "vim.argumentObjectClosingDelimiters": [")", "]", "}"],
  "vim.argumentObjectOpeningDelimiters": ["(", "[", "{"],
  "vim.digraphs": {},
  "csharp.semanticHighlighting.enabled": true,
  "csharp.suppressDotnetInstallWarning": true,
  "csharp.suppressBuildAssetsNotification": true,
  "csharp.suppressDotnetRestoreNotification": true,
  "csharp.suppressProjectJsonWarning": true,
  "omnisharp.useGlobalMono": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "json.maxItemsComputed": 10000,
  "workbench.editorAssociations": {
    "*.log": "default"
  },
  "vim.easymotion": true,
  "workbench.editor.empty.hint": "hidden",
  "workbench.colorTheme": "Default Dark+",
  "markdown.extension.tableFormatter.enabled": false,
  "redhat.telemetry.enabled": false,
  "chat.mcp.discovery.enabled": {
    "claude-desktop": true,
    "windsurf": true,
    "cursor-global": true,
    "cursor-workspace": true
  },
  "editor.inlineSuggest.edits.showCollapsed": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "editor.editContext": false,
  "extensions.autoUpdate": false,
  "editor.wordWrapColumn": 200,
  "godotTools.editorPath.godot4": "/Applications/Godot.app/Contents/MacOS/Godot"
}
